  "author": "Siberia <dev@siberia.io>",
  "contributors": [
    "Thomas Carlson",
    "Bryan Swift <bryan@siberia.io>",
    "Derek Kinsman <derek@siberia.io",
    "Kelly Donahue"
  ],
